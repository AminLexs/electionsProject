{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
            "indent": [2, "tab"],
            "no-tabs": 0,
            "no-console": "off",
            "consistent-return": "off",
            "no-undef": "off",
            "no-underscore-dangle": "off",
            "no-plusplus": "off"

    }
}
